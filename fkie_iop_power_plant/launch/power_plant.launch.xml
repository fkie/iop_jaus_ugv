<launch>
    <arg name="id_subsystem" default="127"/>
    <arg name="id_node" default="64"/>
    <node name="iop_power_plant" pkg="fkie_iop_component" exec="iop_component" _args="--ros-args --log-level debug" output="screen" >
        <param name="jaus_config" value="$(find-pkg-share jaustoolset)/cfg/cmp.cfg"/>
        <param name="iop_address" value="$(var id_subsystem).$(var id_node).65"/>
        <param from="$(find-pkg-share fkie_iop_power_plant)/launch/power_plant_services.yaml"/>
    </node>
</launch>
