<launch>
    <arg name="id_subsystem" default="127"/>
    <arg name="id_node" default="64"/>
    <node name="iop_stabilizer_driver" pkg="fkie_iop_component" exec="iop_component" _args="--ros-args --log-level debug" output="screen" >
        <param name="jaus_config" value="$(find-pkg-share jaustoolset)/cfg/cmp.cfg"/>
        <param name="iop_address" value="$(var id_subsystem).$(var id_node).67"/>
        <param from="$(find-pkg-share fkie_iop_stabilizer_driver)/launch/stabilizer_driver_services.yaml"/>
    </node>
</launch>
